{
  "tabs": [
    {
      "label": "用户管理",
      "name": "user",
      "schemaId": "table",
      "itemSchemas": {
        "tableCols": [
          {
            "label": "姓名",
            "prop": "name",
            "width": ""
          },
          {
            "label": "性别",
            "prop": "sex",
            "width": "200"
          },
          {
            "label": "年龄",
            "prop": "age",
            "width": "200"
          },
          {
            "label": "是否可用",
            "prop": "enable",
            "width": "200"
          }
        ],
        "searchItems": [
          {
            "label": "姓名",
            "prop": "name",
            "type": "text",
            "optionType": "fixed",
            "options": []
          },
          {
            "label": "性别",
            "prop": "sex",
            "type": "select",
            "optionType": "fixed",
            "options": [
              {
                "label": "男",
                "value": "1"
              },
              {
                "label": "女",
                "value": "2"
              }
            ]
          },
          {
            "label": "年龄",
            "prop": "age",
            "type": "text"
          }
        ],
        "hasAdd": true,
        "hasUpdate": true,
        "hasIndex": true,
        "hasPagination": true,
        "addForm": {
          "title": "新增用户",
          "formItems": [
            {
              "label": "姓名",
              "prop": "name",
              "required": true,
              "tips": "\b姓名不可更改",
              "type": "text",
              "maxlength": "2"
            },
            {
              "label": "性别",
              "prop": "sex",
              "required": true,
              "append": "",
              "type": "radioGroup",
              "optionType": "fixed",
              "options": [
                {
                  "label": "男",
                  "value": "1"
                },
                {
                  "label": "女",
                  "value": "2"
                }
              ]
            },
            {
              "label": "年龄",
              "prop": "age",
              "required": true,
              "type": "text",
              "dateType": "date",
              "append": "岁"
            },
            {
              "label": "生日",
              "prop": "birthday",
              "type": "date",
              "required": true,
              "dateType": "datetimerange"
            },
            {
              "label": "爱好",
              "prop": "hobby",
              "type": "checkboxGroup",
              "required": true,
              "optionType": "fixed",
              "options": [
                {
                  "label": "篮球",
                  "value": "1"
                },
                {
                  "label": "唱歌",
                  "value": "2"
                },
                {
                  "label": "游泳",
                  "value": "3"
                }
              ]
            }
          ],
          "api": "/api/user/add",
          "formTransform": "export default function(form) {\n    return {\n        realName: form.name,\n        price: form.price * 100\n    }\n}"
        },
        "updateForm": {
          "title": "编辑用户",
          "formItems": [
            {
              "label": "姓名",
              "prop": "name",
              "required": true,
              "tips": "姓名",
              "type": "text"
            },
            {
              "label": "性别",
              "prop": "sex",
              "required": true,
              "type": "text"
            },
            {
              "label": "年龄",
              "prop": "age",
              "required": true,
              "append": "岁",
              "type": "text"
            }
          ],
          "api": "/api/user/update",
          "formTransform": "export default function(form) {\n    return {\n        realName: form.name,\n        price: form.price * 100\n    }\n}"
        },
        "hasDelete": true,
        "hasBatchDelete": true,
        "hasToggleEnable": true,
        "hasMove": true,
        "enableProp": "enable",
        "deleteApi": "/api/user/delete",
        "batchDeleteApi": "/api/user/batchDelete",
        "toggleEnableApi": "/api/user/toggleEnable",
        "moveApi": "/api/user/move",
        "addFormType": "dialog",
        "dataApi": "/api/user/pageList",
        "queryTransform": "export default function({\n    query,\n    pageSize,\n    pageNum\n}) {\n    return {\n        query: { ...query, category: 1 },\n        pageSize,\n        pageNum\n    }\n}",
        "hasViewDetail": true,
        "viewDetailType": "page",
        "viewDetailFields": [
          {}
        ]
      }
    },
    {
      "label": "项目管理",
      "name": "project",
      "schemaId": "table",
      "itemSchemas": {
        "tableCols": [
          {
            "label": "项目名称",
            "prop": "name"
          }
        ],
        "searchItems": [
          {
            "label": "项目名称",
            "prop": "name",
            "type": "text"
          }
        ],
        "hasIndex": true,
        "hasPagination": true,
        "hasAdd": false,
        "hasUpdate": false,
        "addForm": {
          "formItems": [
            {
              "required": true
            }
          ]
        },
        "updateForm": {
          "formItems": [
            {
              "required": true
            }
          ]
        },
        "hasViewDetail": false,
        "viewDetailType": "dialog",
        "viewDetailFields": []
      }
    }
  ]
}