<script>
    [
        {
            type: "list",
            label: "tabs项",
            prop: "tabs",
            schemas: [
                {type: "text", label: "名称", prop: "label"},
                {type: "text", label: "id", prop: "name"},
                {
                    type: "list",
                    label: "配置组件",
                    width: 80,
                    dialogWidth: 640,
                    schemas: [
                        {
                            type: "select",
                            label: "选择组件",
                            prop: "schemaId",
                            options: [
                                { label: "表格", value: "table" }
                            ],
                            effect({ model, resolveSchemas }) {
                                if (model.schemaId === "table") {
                                    return {
                                        prop: "itemSchemas",
                                        schemas: resolveSchemas("table")
                                    }
                                }
                            }
                        },
                    ]
                }
            ]
        }
    ]
</script>